.nav {
  top: 0;
  left: 0;
  position: fixed;
  box-shadow: 0 5px 10px rgba(191, 191, 191, 0.1);
  height: 40px;
  width: 100%;
  padding: 0 215rpx;
  box-sizing: border-box;
  color: #999;
  font-size: 14px;
  font-weight: 200;
  z-index: 1;
  background: #fff;
}
.nav .navBtn {
  float: left;
  width: 40px;
  line-height: 40px;
  margin-right: 20px;
  text-align: center;
}
.nav .navBtn:last-child {
  margin: 0;
}
.swiper {
  margin-top: 40px;
  height: 250px;
}
.swiper image {
  height: 100%;
  width: 100%;
}

.baseInfo {
  margin-top: 10px;
}
.baseInfo text {
  margin-left: 10px;
}

.sku {
  border-top: solid 10px #f5f5f5;
  padding: 10px 30px 10px 12px;
  position: relative;
  color: #333;
  font-size: 12px;
}
.sku label:last-child {
  position: absolute;
  right: 12px;
  top: 10px;
}

.marketing {
  border-top: solid 10px #f5f5f5;
  padding: 10px 12px;
  box-sizing: border-box;
  color: #333;
  font-size: 12px;
}

.goodsInfo {
  border-top: solid 10px #f5f5f5;
  padding: 5px 12px;
  color: #bbb;
  font-size: 10px;
  font-weight: 200;
}
.goodsInfo .goodsInfoCell {
  padding: 5px 0;
}

.images {
  width: 750rpx;
}
.images image {
  width: 100%;
}
